{
        "languageserver": {
                "rust": {
                        "command": "rust-analyzer",
                        "trace.server": "verbose",
                        "filetypes": [
                                "rs"
                        ],
                        "rootPatterns": [
                                "Cargo.toml",
                                "Cargo.lock"
                        ],
                        "progressOnInitialization": true
                },
                "golang": {
                        "command": "gopls",
                        "rootPatterns": [
                                "go.mod",
                                "go.sum"
                        ],
                        "filetypes": [
                                "go"
                        ]
                },
                "haskell": {
                        "command": "hie-wrapper",
                        "args": [
                                "--lsp"
                        ],
                        "rootPatterns": [
                                "stack.yaml",
                                "cabal.config",
                                "package.yaml"
                        ],
                        "filetypes": [
                                "hs",
                                "lhs",
                                "haskell"
                        ],
                        "initializationOptions": {
                                "languageServerHaskell": {
                                        "hlintOn": true
                                }
                        }
                },
        },
