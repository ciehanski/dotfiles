                "constructor": " îˆ ",
                "value": " ï¢Ÿ ",
                "enumMember": " ï… "
        },
        "coc.preferences.formatOnSave": false,
        "Lua.diagnostics.globals": [
                "vim"
        ],
        "rust-analyzer.enable": true,
        "rust-analyzer.server.path": "/code/.rustup/toolchains/stable-x86_64-unknown-linux-gnu/bin/rust-analyzer",
        "rust-analyzer.inlayHints.typeHints.enable": true,
        "rust-analyzer.inlayHints.chainingHints.enable": true,
        "rust-analyzer.inlayHints.renderColons": false,
        "rust-analyzer.inlayHints.parameterHints.enable": false,
        "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
        "rust-analyzer.restartServerOnConfigChange": true,
        "rust-analyzer.updates.checkOnStartup": false,
        "rust-analyzer.checkOnSave": true,
        "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/16.0.2/clangd_16.0.2/bin/clangd",
}
